<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/searchactivity_history_layout_item"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="@dimen/searchactivity_history_item_height">
    <TextView
        android:id="@+id/history_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/searchlayout_left"
        android:textColor="@color/color_b2ffffff"
        android:textSize="@dimen/sarrs_text_15sp"
        android:singleLine="true"
        android:gravity="center_vertical"
        android:layout_centerVertical="true" />

</RelativeLayout>